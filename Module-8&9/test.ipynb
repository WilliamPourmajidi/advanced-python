    "def compute(val):  \n",
    "    total = 0  \n",
    "    for i in range(val):  \n",
    "        if i % 2 == 0:  \n",
    "            total += i  \n",
    "            print(f\"Adding {i}, total now {total}\")\n",
    "    return total  \n",
    "  \n",
    "print(compute(5))\n"
   ]
  }
